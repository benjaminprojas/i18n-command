{
    "name": "wp-cli/makepot-command",
    "description": "Create POT files for WordPress projects",
    "type": "wp-cli-package",
    "homepage": "https://github.com/wp-cli/makepot-command",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [ "makepot-command.php" ],
        "psr-4": {
            "WP_CLI\\Makepot\\": "src/"
        }
    },
    "require": {
        "wp-cli/wp-cli": "^1.1.0",
        "gettext/gettext": "^4.4"
    },
    "require-dev": {
        "behat/behat": "~2.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "commands": [
            "makepot"
        ],
        "bundled": true
    }
}
